<!-- everything below the hero -->

  <!-- user information -->
  <aside class = "col-md-4">
    <!-- about -->
    <%= render "info_pane", info: "profile" %>

    <%= render "info_pane", info: "photos" %>

    <%= render "info_pane", info: "friends" %>

  </aside><!-- user information -->


  <!--post feed-->
  <div class = "col-md-7 col-md-offset-1">

    <%= render 'posts/post_form' %>

    <% @posts.each do |post| %>
